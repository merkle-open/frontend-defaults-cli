{
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json,md}": ["prettier --write", "git add"]
	},
	"scripts": {
		"prettier": "prettier --write \"**/*.*(js|jsx|ts|tsx|json|md)\""
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"devDependencies": {
		"prettier": "1.19.1",
		"@namics/prettier-config": "1.0.1",
		"husky": "4.2.3",
		"lint-staged": "10.0.8"
	}
}

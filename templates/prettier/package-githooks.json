{
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json,md}": ["prettier --write", "git add"]
	},
	"scripts": {
		"prettier": "prettier --write \"**/*.*(js|jsx|ts|tsx|json|md)\""
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"devDependencies": {
		"prettier": "1.19.1",
		"@namics/prettier-config": "0.4.3",
		"husky": "3.1.0",
		"lint-staged": "9.5.0"
	}
}
